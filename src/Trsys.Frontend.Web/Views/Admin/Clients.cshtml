@{
    ViewData["Title"] = "シークレットキー一覧";
}

<h2>@ViewData["Title"]</h2>
<hr />

<div>
    <h3>検索条件</h3>
    <div>
        <div class="row">
            <div class="d-none d-sm-block col-3">
                <h5 class="my-2">属性</h5>
            </div>
            <div class="col-12 col-sm-9">
                <div class="row">
                    <div class="col-auto mb-2">
                        <a href="#" class="btn btn-outline-primary">Publisher</a>
                    </div>
                    <div class="col-auto mb-2">
                        <a href="#" class="btn btn-outline-primary">Subscriber</a>
                    </div>
                    <div class="col-auto mb-2">
                        <a href="#" class="btn btn-outline-primary">接続中</a>
                    </div>
                    <div class="col-auto mb-2">
                        <a href="#" class="btn btn-outline-primary">有効</a>
                    </div>
                    <div class="col-auto mb-2">
                        <a href="#" class="btn btn-outline-primary">無効</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-3 d-none d-sm-block">
                <h5 class="my-2">テキスト</h5>
            </div>
            <div class="col-12 col-sm-9">
                <input type="text" class="form-control d-block" />
            </div>
        </div>
    </div>
    <hr />
</div>

<h3>検索結果</h3>

<div class="list-group">
    <a href="#" class="list-group-item list-group-item-action">
        <div class="d-flex w-100 justify-content-between">
            <h5 class="mb-1">MT4/OANDA Corporation/811631031/2</h5>
            <small>Publisher</small>
        </div>
        <div>山根さん</div>
        <div><span class="badge badge-warning">無効</span></div>
    </a>
    <a href="#" class="list-group-item list-group-item-action">
        <div class="d-flex w-100 justify-content-between">
            <h5 class="mb-1">MT4/OANDA Corporation/811653730/2</h5>
            <small>Subscriber</small>
        </div>
        <div>大川さん</div>
        <div><span class="badge badge-primary">接続中</span></div>
    </a>
</div>